---
import type { HTMLAttributes } from 'astro/types';
export type Props = HTMLAttributes<'svg'>;
import Logo from '~/assets/logo.webp';
---

<picture>
	<source srcset={Logo.src} />
	<img
		src={Logo.src}
		alt="Bodegas Maquiavelo"
		class="rounded-full border border-theme-base-300 dark:border-theme-base-600"
		width="85"
		height="85"
	/>
</picture>
