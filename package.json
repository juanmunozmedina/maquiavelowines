{
	"name": "maquiavelowines.netlify.app",
	"type": "module",
	"version": "0.0.1",
	"license": "MIT",
	"engines": {
		"node": "20.17.0"
	},
	"packageManager": "pnpm@10.6.4",
	"scripts": {
		"dev:ci": "doppler run pnpm openapi-ts && doppler run astro dev",
		"dev": "astro dev",
		"start": "astro dev",
		"build:ci": "pnpm openapi-ts && pnpm astro build",
		"build": "pnpm astro build",
		"astro": "astro",
		"lint": "astro sync && run-s --continue-on-error lint:*",
		"lint:biome": "biome lint src",
		"lint:astro": "astro check",
		"lint:types": "tsc -b",
		"test:e2e": "playwright test",
		"test:unit": "vitest",
		"format": "run-s --continue-on-error format:*",
		"format:biome": "biome check . --apply",
		"format:prettier": "prettier . --cache --write --list-different"
	},
	"devDependencies": {
		"@astrojs/check": "^0.9.4",
		"@biomejs/biome": "^1.9.4",
		"@hey-api/openapi-ts": "^0.53.12",
		"@types/node": "^20.17.24",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.5.3",
		"prettier-plugin-astro": "^0.12.3",
		"prettier-plugin-jsdoc": "^1.3.2",
		"prettier-plugin-tailwindcss": "^0.5.14",
		"typescript": "^5.8.2",
		"vite": "^5.4.14",
		"vite-plugin-tsconfig-paths": "^1.4.1",
		"vitest": "^2.1.9"
	},
	"dependencies": {
		"@astrojs/netlify": "^6.2.3",
		"@astrojs/partytown": "^2.1.4",
		"@astrojs/sitemap": "^3.2.1",
		"@astrojs/solid-js": "^5.0.5",
		"@fontsource-variable/inter": "5.0.18",
		"@hey-api/client-fetch": "^0.2.4",
		"@iconify-json/mdi": "^1.2.3",
		"@kobalte/core": "0.13.9",
		"@kobalte/tailwindcss": "0.9.0",
		"@playwright/test": "^1.51.0",
		"@sentry/astro": "^8.55.0",
		"@tailwindcss/typography": "0.5.13",
		"@tailwindcss/vite": "^4.0.13",
		"@tanstack/solid-query": "^5.56.2",
		"@tanstack/solid-query-devtools": "^5.56.2",
		"@total-typescript/ts-reset": "^0.6.1",
		"astro": "^5.5.3",
		"astro-breadcrumbs": "^3.3.1",
		"astro-icon": "^1.1.5",
		"flowbite": "^2.5.2",
		"photoswipe": "^5.4.4",
		"solid-icons": "^1.1.0",
		"solid-js": "^1.9.5",
		"stripe": "16.6.0",
		"tailwind-merge": "2.4.0",
		"tailwindcss": "^4.0.13",
		"tailwindcss-animate": "^1.0.7",
		"type-fest": "^4.37.0",
		"vanilla-cookieconsent": "^3.1.0",
		"zod": "3.23.8"
	}
}
